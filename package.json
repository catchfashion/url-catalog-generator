    "@types/chai": "4.1.7",
    "@types/mocha": "2.2.48",
    "@types/node": "^8.10.54",
    "chai": "4.2.0",
    "check-engine": "1.8.1",
    "mocha": "3.5.3",
    "np": "3.1.0",
